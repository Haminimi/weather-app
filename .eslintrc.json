{
	"env": {
		"browser": true,
		"es2021": true
	},
	"extends": [
        "airbnb-base", 
        "prettier"
    ],
	"parserOptions": {
		"ecmaVersion": "latest",
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"linebreak-style": "off",
		"import/no-extraneous-dependencies": "off",
		"import/prefer-default-export": "off",
		"spaced-comment": "off"
	}
}
